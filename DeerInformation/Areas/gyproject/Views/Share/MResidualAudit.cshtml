@model DeerInformation.Models.V_GM_MResidual

@{
    Layout = "~/Views/Shared/_BaseLayout.cshtml";
    var X = Html.X();
}

@section content{
    @(
         X.Panel().Title("余料退回单明细").Layout(LayoutType.HBox).AutoScroll(true).Items
                (
                X.Container().Layout(LayoutType.Anchor).Flex(2).DefaultAnchor("100%").MarginSpec("0 5 0 5").Items(
                    X.TextFieldFor(m => m.ResidualNo).FieldLabel("余料退回单号").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.ProjectNo).FieldLabel("项目编号").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.ResidualType).FieldLabel("退回方式").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.BackDate).FieldLabel("退回日期").ReadOnly(true).Flex(1)),
                X.Container().Layout(LayoutType.Anchor).Flex(2).DefaultAnchor("100%").MarginSpec("0 5 0 5").Items(
                    X.TextFieldFor(m => m.AuditProcess).FieldLabel("审核流程").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.SupplierName).FieldLabel("供应商").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.Name).FieldLabel("经办人").ReadOnly(true).Flex(1),
                    X.TextFieldFor(m => m.Remark).FieldLabel("备注").ReadOnly(true).Flex(1))          
                )      
    )
    @(X.Panel().ID("panel")
    //.Title("材料退回单物料明细")
)
}


    
