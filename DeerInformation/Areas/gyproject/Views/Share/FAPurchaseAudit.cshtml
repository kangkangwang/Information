@model DeerInformation.Models.V_GM_FAPurchase

@{
    Layout = "~/Views/Shared/_BaseLayout.cshtml";
    var X = Html.X();
}

@section content{
   @(
        X.Panel().Title("固定资产采购单明细").Layout(LayoutType.HBox).AutoScroll(true).Items
            (
                X.Container().Layout(LayoutType.Anchor).Flex(2).DefaultAnchor("100%").MarginSpec("0 5 0 5").Items(
                        X.TextFieldFor(m => m.PurchaseFNo).FieldLabel("采购单号").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.SupplierName).FieldLabel("供应商").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.ApplyNo).FieldLabel("申请单号").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.No_Date).FieldLabel("预交日").ReadOnly(true).Flex(1).LabelWidth(60)
                ),
                X.Container().Layout(LayoutType.Anchor).Flex(2).DefaultAnchor("100%").MarginSpec("0 5 0 5").Items(
                        X.TextFieldFor(m => m.AuditProcess).FieldLabel("审核流程").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.Status).FieldLabel("采购状态").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.OrderType).FieldLabel("订单类型").ReadOnly(true).Flex(1).LabelWidth(60),
                        X.TextFieldFor(m => m.Name).FieldLabel("经办人").ReadOnly(true).Flex(1).LabelWidth(60)
                )
            )      
    )
    @(X.Panel().ID("panel"))
}


    
