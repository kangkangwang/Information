@model DeerInformation.Models.V_GM_FAStorage

@{
    Layout = "~/Views/Shared/_BaseLayout.cshtml";
    var X = Html.X();
}

@section content{
    @(
         X.Panel().Title("固定资产入库单明细").Layout(LayoutType.HBox).AutoScroll(true).Items
        (
            X.Container().Layout(LayoutType.Anchor).DefaultAnchor("100%").MarginSpec("0 5 0 5").Flex(1).Items(
                X.TextFieldFor(m => m.StorageNo).FieldLabel("入库单号").ReadOnly(true).Flex(1),
                X.TextFieldFor(m=>m.PurchaseFNo).FieldLabel("采购单号").ReadOnly(true).Flex(1),
                X.TextFieldFor(m => m.WarehouseName).FieldLabel("入库仓库").ReadOnly(true).Flex(1)),
            X.Container().Layout(LayoutType.Anchor).DefaultAnchor("100%").MarginSpec("0 5 0 5").Flex(1).Items(
                X.TextFieldFor(m => m.StorageType).FieldLabel("入库类型").ReadOnly(true).Flex(1),
                X.TextFieldFor(m => m.AuditProcess).FieldLabel("审核流程").ReadOnly(true).Flex(1),
                X.TextFieldFor(m => m.No_Date).FieldLabel("入库日期").ReadOnly(true).Flex(1))
        )      
    )
    @(X.Panel().ID("panel"))
}
